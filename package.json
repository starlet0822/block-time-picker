{
  "name": "block-time-picker",
  "version": "1.0.3",
  "type": "module",
  "description": "A time-picker base on vue3.x",
  "keywords": [
    "block-time-picker",
    "time-picker",
    "vue-component",
    "vue3",
    "vite"
  ],
  "license": "MIT",
  "author": "wuxiaoxing",
  "homepage": "https://starlet0822.github.io/block-time-picker/tree/main/#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/starlet0822/block-time-picker.git"
  },
  "bugs": {
    "url": "https://github.com/starlet0822/block-time-picker/issues"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./dist/index.umd.js",
  "module": "./dist/index.es.js",
  "unpkg": "dist/index.umd.js",
  "jsdelivr": "dist/index.umd.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js",
      "require": "./dist/index.umd.js"
    },
    "./package.json": "./package.json",
    "./*": "./*"
  },
  "files": [
    "dist",
    "src/components"
  ],
  "style": "dist/style.css",
  "scripts": {
    "i": "pnpm install",
    "dev": "vite",
    "build": "vite build",
    "build:lib": "vite build",
    "build:netlify": "vite build --mode netlify",
    "preview": "vite preview",
    "patch:version": "npm version patch",
    "publish:lib": "npm publish",
    "clean:lib": "rimraf node_modules",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean": "pnpm clean:lib && pnpm clean:cache",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "deploy": "pnpm run build && gh-pages -d dist"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.3.1",
    "dayjs": "^1.11.13",
    "element-plus": "^2.9.7",
    "lodash-es": "^4.17.21",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.21.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/eslint-config-prettier": "^10.2.0",
    "autoprefixer": "10.4.17",
    "eslint": "^9.21.0",
    "eslint-plugin-vue": "~10.0.0",
    "gh-pages": "^6.3.0",
    "globals": "^16.0.0",
    "postcss": "8.4.35",
    "prettier": "3.5.3",
    "sass": "^1.86.0",
    "tailwindcss": "3.4.1",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-vue-components": "^28.4.1",
    "vite": "^6.2.1",
    "vite-plugin-remove-console": "^2.2.0",
    "vite-plugin-vue-devtools": "^7.7.2"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  }
}
